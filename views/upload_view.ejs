<%- include('partials/head') %>
<div class="container-center">
   <a href="/" onClick="history.back(); return false;">Back</a>
    <% upload_item.picture = upload_item.picture || upload_item.attachment %>
    <% upload_item.pictures = upload_item.pictures || upload_item.attachments %>

	<% if (upload_item.picture) { %>
	    <% upload_item.pictures.forEach(picture => { %>
              <div class="picture-item">
                <img class="img img-rounded w-100" src="<%= picture.uri %>" title="<%= picture.title %>" data-name="<%= picture.title %>" />
              </div>
    	    <% }) %>
	<% } else { %>
          <div class="alert alert-info">No pictures to show.</div>
	<% } %>
    </form>
</div>
<%- include('partials/footer') %>
