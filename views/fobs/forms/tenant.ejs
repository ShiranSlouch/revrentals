<form class="form-horizontal" method="POST">
<fieldset>

<!-- Form Name -->
<legend>Form Name</legend>

<!-- Text input-->
<div class="form-group">
  <label class="col-md-4 control-label" for="full_name">Full Name</label>  
  <div class="col-md-5">
  <input value="<%= item.full_name %>" id="full_name" name="full_name" type="text" placeholder="" class="form-control input-md" required="">
  <span class="help-block">Enter Full Name</span>  
  </div>
</div>

<!-- Text input-->
<div class="form-group">
  <label class="col-md-4 control-label" for="Email">Email Address</label>  
  <div class="col-md-5">
  <input value="<%= item.email %>" id="Email" name="Email" type="email" placeholder="" class="form-control input-md" required="">
  <span class="help-block">Enter Email Address</span>  
  </div>
</div>

<!-- Text input-->
<div class="form-group">
  <label class="col-md-4 control-label" for="phone">Phone Number</label>  
  <div class="col-md-5">
  <input value="<%= item.phone_number %>" id="phone" name="phone_number" type="text" placeholder="" class="form-control input-md" required="">
  <span class="help-block">Enter Phone Number</span>  
  </div>
</div>

    
<!-- Button (Double) -->
<div class="form-group">
  <label class="col-md-4 control-label"></label>
  <div class="col-md-8">
    <button class="btn btn-success">Save</button>
    <a href="/fobs/tenants" class="btn btn-warning">Cancel</a>
  </div>
</div>

</fieldset>
</form>
